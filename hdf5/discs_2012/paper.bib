@article{mahinthakumar02,
	Abstract = {The hybrid MPI-OpenMP model is a natural parallel programming paradigm
	for emerging parallel architectures that are based on symmetric multiprocessor
	(SMP) clusters. This paper presents a hybrid implementation adapted
	for an implicit finite-element code developed for groundwater transport
	simulations. The original code was parallelized for distributed memory
	architecturesusing MPI (Message Passing Interface) using a domain
	decomposition strategy. OpenMP directives were then added to the
	code (a straightforward loop-level implementation) to use multiple
	threads within each MPI process. To improve the OpenMP performance,
	several loop modifications were adopted. The parallel performance
	results are compared for four modern parallel architectures. The
	results show that for most of the cases tested, the pure MPI approach
	outperforms the hybrid model. The exceptions to this observation
	were mainly due to a limitation in the MPI library implementation
	on one of the architectures. A general conclusion is that while the
	hybrid model is a promising approach for SMP cluster architectures,
	at the time of this writing, the payoff may not be justified for
	converting all existing MPI codes to hybrid codes. However, improvements
	in OpenMP compilers combined with potentialMPI limitations in SMP
	nodes may make the hybrid approach more attractive for a broader
	set of applications in the future.},
	Author = {Mahinthakumar, G and Saied, F},
	Eprint = {http://hpc.sagepub.com/cgi/reprint/16/4/371.pdf},
	Journal = {International Journal of High Performance Computing Applications},
	Number = {4},
	Pages = {371-393},
	Title = {A Hybrid {MPI-OpenMP} Implementation of an Implicit Finite-Element  Code on Parallel Architectures},
	Url = {http://hpc.sagepub.com/cgi/content/abstract/16/4/371},
	Volume = {16},
	Year = {2002},
	Bdsk-Url-1 = {http://hpc.sagepub.com/cgi/content/abstract/16/4/371}
}

